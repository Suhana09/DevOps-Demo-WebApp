pipeline {
    agent any
    environment {
        PATH = "/opt/maven3.6.3/bin:$PATH"
    }
    stages{
        stage("Git"){
            steps{
                git 'https://github.com/Suhana09/DevOps-Demo-WebApp.git'
            }
        }
        stage("Maven Build") {
            steps {
                sh "mvn clean package"
            }
        }
    }
}
