pipeline {
    agent any
    stages {
        stage("Git"){
            steps{
                git 'https://github.com/Suhana09/DevOps-Demo-WebApp.git'
            }
        }
        stage("Jira") {
            steps {
                jiraIssueSelector(issueSelector: [$class: 'ExplicitIssueSelector', issueKeys: 'DT-3'])
            }
            
        }
    }
}
